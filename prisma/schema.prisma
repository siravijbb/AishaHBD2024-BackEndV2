generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model DonateData {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  TotalDonated Float
  TotalNeed    Float
  ids          String
}

model FormConfig {
  id               String @id @default(auto()) @map("_id") @db.ObjectId
  ReadableDateTime String
  formClose        String
}

model InvitedUser {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  ids        String
  invitecode String
}

model OverrideAccept {
  id     String  @id @default(auto()) @map("_id") @db.ObjectId
  status Boolean
}

model Wishes {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  DMY           String
  agreed        String
  approved      Boolean
  comment       String
  /// Multiple data types found: String: 88.9%, Int: 11.1% out of 18 sampled entries
  count         Json
  lowercasename String
  name          String
  picture       String
  sidepicture   String
  stattime      String
  time          String
}
