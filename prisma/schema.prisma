generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model DonateData {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  ids          String
  TotalNeed    Float
  TotalDonated Float
}

model InvitedUser {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  ids        String
  invitecode String @unique
}
